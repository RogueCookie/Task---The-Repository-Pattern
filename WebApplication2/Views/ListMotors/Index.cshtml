@model IEnumerable<WebApplication2.Models.ListMotor>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Motor_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Max_power__kW_)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Voltage__V_)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Current__A_)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fuel_consumtion_per_hour_l_h_)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Max_torque_at__rpm_)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Max_presure__bar_)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Displacement__cm3_rev_)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Motor_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Max_power__kW_)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Voltage__V_)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Current__A_)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fuel_consumtion_per_hour_l_h_)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Max_torque_at__rpm_)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Max_presure__bar_)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Displacement__cm3_rev_)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
