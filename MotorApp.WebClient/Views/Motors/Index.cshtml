@model IEnumerable<MotorApp.DataAccess.Motor>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TimeStamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MotorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Current)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DifferenceC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Revs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DifferenceR)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pressure)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DifferenceP)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TimeStamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MotorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Current)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DifferenceC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Revs)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DifferenceR)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pressure)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DifferenceP)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
